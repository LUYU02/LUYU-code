/* pages/movie/movie.wxss */
.box_cell{
  height: 200rpx;
  padding: 10rpx;
  display: flex;
  justify-content: space-between;
  border-top:1rpx solid #ccc;
  border-bottom:1rpx solid #ccc;
}
.right_box{
  display: flex;
  align-items: center;
}
.img_box{
  width: 180rpx;
}
.img_box image{
  width: 100%;
  height: 100%;
}
.center{
  width: 360rpx;
  padding: 10rpx 20rpx;
  overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;    
}
.center view {
  margin: 10rpx 0;
}